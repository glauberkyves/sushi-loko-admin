<div class="btn-group  btn-group-sm">
    {% if data.stAtivo %}
        <a href="{{ path('super_franquia_transacao', {id: data.idTransacao}) }}" class="btn btn-success btn-xs">
            <span class="fa fa-check"></span>
        </a>
    {% else %}
        <a href="{{ path('super_franquia_transacao', {id: data.idTransacao}) }}" class="btn btn-danger btn-xs">
            <span class="fa  fa-warning"></span>
        </a>
    {% endif %}
</div>